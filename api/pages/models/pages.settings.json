{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Pages"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name",
      "required": true
    },
    "Conatact": {
      "type": "richtext",
      "required": true
    },
    "Thembnal": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "Excerpt": {
      "type": "text",
      "required": true
    },
    "tags": {
      "collection": "tags",
      "via": "pages"
    },
    "authors": {
      "collection": "author",
      "via": "pages"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "published",
        "draft"
      ],
      "default": "draft",
      "required": true
    }
  }
}
