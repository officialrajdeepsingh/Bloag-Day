{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Posts"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Contant": {
      "type": "richtext",
      "required": true,
      "minLength": 20
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "categories": {
      "collection": "categories",
      "via": "posts"
    },
    "Thembnal": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "Excerpt": {
      "type": "text",
      "required": true
    },
    "tags": {
      "collection": "tags",
      "via": "posts"
    },
    "authors": {
      "collection": "author",
      "via": "posts"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "published",
        "draft"
      ],
      "default": "draft",
      "required": true
    }
  }
}
